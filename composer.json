{
    "name": "thecrypticace/hooks",
    "description": "PHPUnit test hooks",
    "keywords": ["phpunit", "test", "hooks", "before", "after"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Pittman",
            "email": "jordan@cryptica.me"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "TheCrypticAce\\Hooks\\": "src/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^3.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
